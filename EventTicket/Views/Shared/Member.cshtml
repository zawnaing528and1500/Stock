
@using System.Data;
@using EventTicket.App_Code;
@{
    Base b = new Base();string ActiveClass = "",ActiveText="";
    int MemberID = Convert.ToInt32(Session["CurrentUserID"]);
    Boolean Active = b.getBooleanByQuery("select * from Member where ID="+MemberID, "Active");
    if (Active)
    {
        ActiveClass = "fa fa-circle text-success";
        ActiveText = "Work";
    }
    else {
        ActiveClass = "fa fa-circle text-danger";
        ActiveText = "Don't Work";
    }
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Myanmar IT Star Company Limited</title>
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <link rel="stylesheet" href="~/Content/bootstrap.min.css">
    <link rel="stylesheet" href="~/Content/font-awesome.min.css">
    <link rel="stylesheet" href="~/Content/ionicons.min.css">
    <link rel="stylesheet" href="~/admin-lte/css/AdminLTE.css">
    <link rel="stylesheet" href="~/admin-lte/css/skins/skin-green.min.css">
    <link rel="stylesheet" href="~/Content/jquery-ui.css">
    <link rel="stylesheet" href="~/Content/select2.min.css">
    <link rel="stylesheet" href="~/Content/Site.css">
    <link rel="stylesheet" href='https://mmwebfonts.comquas.com/fonts/?font=zawgyi' />
    <link rel="stylesheet" href="~/Content/dataTables.bootstrap.min.css">
    <script src="~/Scripts/jquery.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/admin-lte/js/adminlte.min.js"></script>
    <script src="~/Scripts/bootstrap-datepicker.min.js"></script>
    <script src="~/Scripts/select2.full.min.js"></script>
    <script src="~/Scripts/dataTables.bootstrap.min.js"></script>
    <script src="~/Scripts/jquery.dataTables.min.js"></script>
    <script src="~/Scripts/Site.js"></script>
    <script>
    //Date picker
    $('#datepicker').datepicker({
    autoclose: true
    })
    $('#Todatepicker').datepicker({
    autoclose: true
    })
    </script>
</head>
<body class="hold-transition skin-green sidebar-mini">
    <div class="loader"></div>
    <div class="wrapper">
        <header class="main-header">
            <a href="#" class="logo">
                <span class="logo-mini"><b>N</b></span>
                <span class="logo-lg"><b>Naing.Org</b></span>
            </a>
            <nav class="navbar navbar-static-top" role="navigation">
                <a href="#" class="sidebar-toggle" data-toggle="push-menu" role="button">
                    <span class="sr-only">Toggle navigation</span>
                </a>
                @*Notification*@
            </nav>
        </header>
        <aside class="main-sidebar">
            <section class="sidebar">
                <div class="user-panel">
                    <div class="pull-left image">
                        <img src="~/Image/profile.png" class="img-circle" alt="User Image" width="45px" height="45px">
                    </div>
                    <div class="pull-left info">
                        <p>@b.getStringByQuery("select * from Member where ID="+MemberID,"Name")</p>
                        <!-- Status -->
                        <a href="#"><i class="@ActiveClass"></i> @ActiveText</a>
                    </div>
                </div>
                <!-- /.search form -->
                <!-- Sidebar Menu -->
                <ul class="sidebar-menu" data-widget="tree">
                    <li class="header"><b><span class="zg">ျမန္​မာအိုင္​တီစတား ကုမၸဏီ လီမိတက္</span>​</b></li>
                    <li><a href="~/Member/Dashboard"><i class="fa fa-dashboard" aria-hidden="true"></i><span class="zg">Dashboard</span></a></li>
                    <li><a href="~/Member/AddShortLink"><i class="fa fa-file-text" aria-hidden="true"></i><span class="zg">Add Shorten Link</span></a></li>
                    <li><a href="~/Member/MyReferralLink"><i class="fa fa-link" aria-hidden="true"></i><span class="zg">My Referral Link</span></a></li>
                    <li><a href="~/Member/GetBrowser"><i class="fa fa-link" aria-hidden="true"></i><span class="zg">Browser ယူရန္​</span></a></li>
                    <li class="treeview">
                        <a href="#">
                            <i class="fa fa-users"></i> <span class="zg">Under Me</span>
                            <span class="pull-right-container">
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="~/Member/MyReferralByLevel?Level=1"><i class="fa fa-circle-o"></i><span class="zg">Stage 1​</span></a></li>
                            <li><a href="~/Member/MyReferralByLevel?Level=2"><i class="fa fa-circle-o"></i><span class="zg">Stage 2</span></a></li>
                            <li><a href="~/Member/MyReferralByLevel?Level=3"><i class="fa fa-circle-o"></i><span class="zg">Stage 3​</span></a></li>
                        </ul>
                    </li>
                    <li class="treeview">
                        <a href="#">
                            <i class="fa fa-history"></i> <span class="zg">History</span>
                            <span class="pull-right-container">
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            <li class="treeview">
                                <a href="#">
                                    <i class="fa fa-money"></i> <span class="zg">Transfer</span>
                                    <span class="pull-right-container">
                                        <i class="fa fa-angle-left pull-right"></i>
                                    </span>
                                </a>
                                <ul class="treeview-menu">
                                    <li><a href="~/Member/TransferHistory?Type=1"><i class="fa fa-circle-o"></i><span class="zg">Send</span></a></li>
                                    <li><a href="~/Member/TransferHistory?Type=2"><i class="fa fa-circle-o"></i><span class="zg">Receive</span></a></li>
                                  </ul>
                            </li>
                            <li><a href="~/Member/WithdrawHistory"><i class="fa fa-circle-o"></i><span class="zg">Withdraw</span></a></li>
                            <li><a href="~/Image/construction.png"><i class="fa fa-circle-o"></i><span class="zg">Deposit</span></a></li>
                        </ul>
                    </li>
                    <li><a href="~/Member/Setting"><i class="fa fa-cog" aria-hidden="true"></i><span class="zg">Setting</span></a></li>
                    @*<li><a href="/Shop/CloseAccount"><i class="fa fa-book" aria-hidden="true"></i><span class="zg">Close Account</span></a></li>*@
                    <li><a href="/Member/Aggreement"><i class="fa fa-book" aria-hidden="true"></i><span class="zg">သ​ေဘာတူညီမႈ​</span></a></li>
                     <li><a href="~/Account/Logout"><i class="fa fa-power-off" aria-hidden="true"></i><span>Log Out</span></a></li>
                </ul>
                <!-- /.sidebar-menu -->
            </section>
            <!-- /.sidebar -->
        </aside>

        <div class="content-wrapper">
            <section class="content container-fluid">
                @RenderBody()
            </section>
        </div>
        <footer class="main-footer">
            <!-- To the right -->
            <div class="pull-right hidden-xs">

            </div>
            <!-- Default to the left -->
            <strong>Copyright &copy; 2019 <a href="#">MyanmarITStar Co.Ltd</a>.</strong>
        </footer>
        <!-- Control Sidebar -->
        <aside class="control-sidebar control-sidebar-dark">
            <!-- Create the tabs -->
            <ul class="nav nav-tabs nav-justified control-sidebar-tabs">
                <li class="active"><a href="#control-sidebar-home-tab" data-toggle="tab"><i class="fa fa-home"></i></a></li>
                <li><a href="#control-sidebar-settings-tab" data-toggle="tab"><i class="fa fa-gears"></i></a></li>
            </ul>
            <!-- Tab panes -->
            <div class="tab-content">
                <!-- Home tab content -->
                <div class="tab-pane active" id="control-sidebar-home-tab">
                    <h3 class="control-sidebar-heading">Recent Activity</h3>
                    <ul class="control-sidebar-menu">
                        <li>
                            <a href="javascript:;">
                                <i class="menu-icon fa fa-birthday-cake bg-red"></i>
                                <div class="menu-info">
                                    <h4 class="control-sidebar-subheading">Langdon's Birthday</h4>
                                    <p>Will be 23 on April 24th</p>
                                </div>
                            </a>
                        </li>
                    </ul>
                    <!-- /.control-sidebar-menu -->
                    <h3 class="control-sidebar-heading">Tasks Progress</h3>
                    <ul class="control-sidebar-menu">
                        <li>
                            <a href="javascript:;">
                                <h4 class="control-sidebar-subheading">
                                    Custom Template Design
                                    <span class="pull-right-container">
                                        <span class="label label-danger pull-right">70%</span>
                                    </span>
                                </h4>
                                <div class="progress progress-xxs">
                                    <div class="progress-bar progress-bar-danger" style="width: 70%"></div>
                                </div>
                            </a>
                        </li>
                    </ul>
                    <!-- /.control-sidebar-menu -->
                </div>
                <!-- /.tab-pane -->
                <!-- Stats tab content -->
                <div class="tab-pane" id="control-sidebar-stats-tab">Stats Tab Content</div>
                <!-- /.tab-pane -->
                <!-- Settings tab content -->
                <div class="tab-pane" id="control-sidebar-settings-tab">
                    <form method="post">
                        <h3 class="control-sidebar-heading">General Settings</h3>
                        <div class="form-group">
                            <label class="control-sidebar-subheading">
                                Report panel usage
                                <input type="checkbox" class="pull-right" checked>
                            </label>
                            <p>
                                Some information about this general settings option
                            </p>
                        </div>
                        <!-- /.form-group -->
                    </form>
                </div>
                <!-- /.tab-pane -->
            </div>
        </aside>
        <div class="control-sidebar-bg"></div>
    </div>
